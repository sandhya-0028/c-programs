#include <stdio.h>
int main()
{
    int N;
    int A = 0;
    int i = 0;
    printf("enter the value of N:");
    scanf("%d",&N);
    while (N != 0) {
        if (N % 10 == 0)
            A = A + 1 * pow(10, i);
        else
            A = A + (N % 10) * pow(10, i);
        N = N / 10;
        i++;
    }
    printf("%d", A);
    return 0;
}
